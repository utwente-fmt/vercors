field f1: Ref

field f2: Ref

method test1(a: Ref)
    requires acc(a.f2, write)
    ensures [true, acc(a.f2, write - perm(a.f1))]
    ensures [true, (forperm r: Ref [r.f2] :: false)]
{
}

method test2(a: Ref)
{
    inhale acc(a.f2, write)
    exhale acc(a.f2, write - perm(a.f1))
    exhale (forperm r: Ref [r.f2] :: false)
}
