field val : Int 

function foo(x: Ref) : Int
  requires let y == (x) in acc(y.val)
  {
    x.val
  }

method test(x:Ref, z:Ref)
  requires acc(x.val) && acc(z.val)
{
  x.val := 4
  assert foo(x) == 4
  z.val := 3
  assert foo(x) == 4
  x.val := 5
  //:: ExpectedOutput(assert.failed:assertion.false)
  assert foo(x) == 42
}
