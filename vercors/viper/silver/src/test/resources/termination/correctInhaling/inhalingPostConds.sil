import "../decreaseOrder/int_decreases.sil"

function f1(x: Int):Int
ensures result > 5

function f2(x: Int): Int
decreases x
{
      x > f1(x) ? f2(x-1) : 3
}

function f3(x: Int): Int
ensures result > 5

function f4(x: Int): Int
decreases x
{
      (f1(x+1) <5 || f3(x-1) > 5) ? f1(f3(x)): f4(x)
}

function f5(x: Int): Int
ensures result > 5
ensures result < 8
ensures result != 7

function f6(x: Int): Int
decreases x
{
      f5(x) == 6 ? 1 : f6(x)
}