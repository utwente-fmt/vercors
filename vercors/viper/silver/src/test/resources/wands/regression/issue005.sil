method test01(x: Ref)
  requires acc(x.f) --* acc(x.g)
  requires acc(x.g) --* acc(x.h)
{
  package (acc(x.f)) --* acc(x.h) {
      apply acc(x.f) --* acc(x.g)
      apply acc(x.g) --* acc(x.h)
  }

  exhale acc(x.f) --* acc(x.h)

