//:: IgnoreFile(/carbon/issue/216/)
field f: Int

method test01(x: Ref, y: Ref, z: Ref)
{
  inhale acc(x.f, 1/2)
  inhale acc(y.f, 1/2)
  inhale acc(z.f)
  assume x == y

  package true --* true {
    assert z != x
    exhale acc(x.f)
    inhale acc(x.f, 1/2)
    inhale acc(y.f, 1/2)
    package true --* true {
      exhale acc(y.f)
    }
  }
}

method test02(x: Ref, z: Ref)
{
  inhale acc(x.f, 1/2)
  inhale x != z ==> acc(x.f, 1/2)
  package true --* true {
    inhale acc(z.f)
    exhale acc(x.f)
  }
}

method test03(x: Ref)
{
  inhale acc(x.f, 1/3)
  package true --* true {
    inhale acc(x.f, 1/3)
    package true --* true {
      inhale acc(x.f, 1/3)
      exhale acc(x.f)
    }
  }
}

method test04(x: Ref, y: Ref)
{
  package true --* true {
    inhale acc(x.f)
    inhale acc(y.f)
    assert x != y
  }
}

method test05(x: Ref, y: Ref)
{
  package true --* true {
    inhale acc(x.f, 1/2)
    inhale acc(y.f, 1/2)
    assume x.f == 4 && y.f == 5
    assert x != y
  }
}