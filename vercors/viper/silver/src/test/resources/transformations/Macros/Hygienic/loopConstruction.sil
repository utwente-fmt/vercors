define increaseFive(x) {
    var oldX: Int := x
    var ctr: Int := 0
    label loop invariant ctr < 5 && x == oldX + ctr
    x := x + 1
    ctr := ctr + 1
    if(ctr < 5) {
        goto loop
    }
    assert(ctr == 5)
    assert(oldX + 5 == x)
}

method main()
{
    var value:Int := 0
    increaseFive(value)
    increaseFive(value)
    assert(value == 10)
}