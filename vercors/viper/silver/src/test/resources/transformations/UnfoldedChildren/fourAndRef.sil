method main() returns ()
{
   var x:Bool := true
   var y:Bool := true
   var r:Bool
   r := fourAnd(x, y, true, true)
   assert(r)
   r := fourAnd(false, false, false, false)
   assert(!r)
   r := fourAnd(false, false, false, false)
   assert(!r)
}

function fourAnd(a: Bool, b: Bool, c: Bool, d:Bool): Bool
{
    a && b && c && d
}