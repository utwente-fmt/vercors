class Role {

	int x;
	
	Role(int v) {
		x = v;
	}
	
	void run() {
		x = 4;
		m1();
	}
	
	void m1() {
		x++;
		m2();
	
	}
	
	void m2() {
		x = x+2;
	}
	
	
}
