class Main {

	Role a,b,c;

	Main() {
		a = new Role(5);
		b = new Role(6);
		c = new Role(7);
	}
	
	void run() {
		if(a.x >= 5 && b.x == 6){
			a.x = 5;
		} else {
			c.x = 10;
		}
	}
	
	void main() {
		Main m = new Main();
		m.run();
	}

}

class Role {

	int x;
	
	Role(int v) {
		x = v;
	}

}
