// -*- tab-width:4 ; indent-tabs-mode:nil -*-
//:: cases TicTacToeInt TicTacToeMatrix
//:: tools session
//:: verdict Pass
class Move {
	int i, j, token;
	
	ensures Perm(i,read) ** Perm(j,read) ** Perm(token,read);
	ensures i == mi ** j == mj ** token == mt;
	Move(int mi, int mj, int mt) {
		i = mi;
		j = mj;
		token = mt;
	}
}
