class parameters {

  given    frac p;
  requires c!=null ** Perm(c.x,p);
  yields   frac q;
  ensures  c!=null ** Perm(c.x,q) ** q <= p;
  void drop(Counter c){
    q = p \ 2;
  }

  void main(){
    Counter c=new Counter();
    c.x=37;
    drop given { p = 1 } (c);
    c.x=12;
  }
}
